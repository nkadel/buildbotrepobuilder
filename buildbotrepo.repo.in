[buildbotrepo]
name=Buildbot $releasever - $basearch
baseurl=file://@@@REPOBASEDIR@@@/buildbotrepo/$releasever/$basearch
enabled=0
gpgcheck=0
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-schol
# Set very, very short for RPM building
metadata_expire=1

[buildbotrepo-source]
name=Buildbot $releasever - Source
baseurl=file://@@@REPOBASEDIR@@@/buildbotrepo/$releasever/SRPMS
enabled=0
gpgcheck=1
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-schol
# Set very, very short for RPM building
metadata_expire=1
